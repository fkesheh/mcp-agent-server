    "start:sse": "node dist/sse.js",
    "test-agent": "npm run build && node dist/cli.js test-agent"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.10",
    "@modelcontextprotocol/sdk": "1.0.1",
    "express": "^4.21.1",
