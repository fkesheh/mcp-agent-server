{
  "name": "mcp-agent-server",
  "version": "1.0.1",
  "description": "A Model Context Protocol (MCP) server that provides AI agents as tools for seamless integration with MCP clients",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai-agents",
    "anthropic",
    "openai",
    "claude",
    "gpt",
    "tools",
    "automation",
    "chatbot",
    "llm"
  ],
  "license": "MIT",
  "author": "Foad M. K. (https://github.com/foadmk)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foadmk/mcp-agent-server.git"
  },
  "homepage": "https://github.com/foadmk/mcp-agent-server",
  "bugs": "https://github.com/foadmk/mcp-agent-server/issues",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "mcp-agent-server": "dist/cli.js"
  },
  "files": [
    "dist",
    "agents-config.json",
    "example-config.json",
    "example-selective-expose.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "serve": "node dist/cli.js serve",
    "test-agent": "node dist/cli.js test-agent"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^3.0.7",
    "@ai-sdk/anthropic": "^2.0.1",
    "@ai-sdk/azure": "^2.0.13",
    "@ai-sdk/cerebras": "^1.0.7",
    "@ai-sdk/cohere": "^2.0.3",
    "@ai-sdk/deepinfra": "^1.0.7",
    "@ai-sdk/deepseek": "^1.0.7",
    "@ai-sdk/fireworks": "^1.0.7",
    "@ai-sdk/google": "^2.0.6",
    "@ai-sdk/google-vertex": "^3.0.8",
    "@ai-sdk/groq": "^2.0.8",
    "@ai-sdk/mistral": "^2.0.4",
    "@ai-sdk/openai": "^2.0.9",
    "@ai-sdk/perplexity": "^2.0.3",
    "@ai-sdk/replicate": "^1.0.3",
    "@ai-sdk/togetherai": "^1.0.7",
    "@ai-sdk/xai": "^2.0.7",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "ai": "^5.0.10",
    "express": "^4.21.1",
    "mcp-ai-agent": "1.0.0",
    "yargs": "^17.7.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/yargs": "^17.0.32",
    "shx": "^0.3.4",
    "typescript": "^5.6.2"
  }
}
