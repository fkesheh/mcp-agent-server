    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/yargs": "^17.0.32",
    "shx": "^0.3.4",
    "typescript": "^5.6.2"
  }
}